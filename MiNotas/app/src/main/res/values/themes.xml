<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MiNotas" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="spin" parent="Theme.MiNotas">
        <item name="android:background">@drawable/add1</item>
    </style>
    <style name="listNota" parent="Theme.MiNotas">
        <item name="android:background">@color/nota</item>
    </style>
    <style name="listTarea" parent="Theme.MiNotas">
        <item name="android:background">@color/cyan</item>
    </style>
    <style name="notasimple" parent="Theme.MiNotas">
        <item name="android:background">@drawable/nota</item>
    </style>
    <style name="buscar" parent="Theme.MiNotas">
        <item name="android:backgroundTint">@drawable/lupa</item>
    </style>
</resources>